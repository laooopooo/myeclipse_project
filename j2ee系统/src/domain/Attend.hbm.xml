<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="domain">
	<class name="Attend" table="attend_inf">
		<cache usage="read-write" />

		<id name="id" type="integer" column="attend_id">
			<generator class="identity"></generator>
		</id>
		<property name="dutyDay" column="duty_day" type="string"
			not-null="true" length="50"></property>
		<many-to-one name="employee" class="Employee" column="employee_id"
			not-null="true" lazy="false"></many-to-one>
		<property name="isCome" column="is_come" type="boolean"
			not-null="true"></property>
		<many-to-one name="type" class="AttendType" column="attendType_id"
			not-null="true" lazy="false"></many-to-one>
		<property name="punchTime" column="punch_time" type="java.util.Date"></property>
	</class>
</hibernate-mapping>
