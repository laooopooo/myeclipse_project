<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="domain">
	<class name="CheckBack" table="checkBack_inf">
		<cache usage="read-write" />
		<id name="id" column="checkBack_id" type="integer">
			<generator class="identity"></generator>
		</id>

		<property name="result" column="check_result" type="boolean"></property>
		<property name="reason" column="check_reason" type="string"
			length="50"></property>
		<many-to-one name="app" class="Application" column="app_id"
			unique="true" not-null="true" lazy="false"></many-to-one>
		<many-to-one name="manager" class="Manager" column="manager_id"
			not-null="true" lazy="false"></many-to-one>
	</class>
</hibernate-mapping>
