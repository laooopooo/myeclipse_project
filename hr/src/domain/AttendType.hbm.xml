<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="domain">
	<class name="AttendType" table="type_table">

		<cache usage="read-only" />
		
		<id name="id" type="integer" column="type_id">
			<generator class="identity"></generator>
		</id>
		
		<property name="name" type="string" column="type_name"
			length="50" not-null="true"></property>
		<property name="amerce" type="double" column="amerce_amount"
			not-null="true"></property>
	</class>
</hibernate-mapping>
