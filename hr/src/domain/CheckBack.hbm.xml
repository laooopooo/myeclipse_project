<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="domain">
	<class name="CheckBack" table="check_table">
		<cache usage="read-only" />

		<id name="id" type="integer" column="check_id">
			<generator class="identity"></generator>
		</id>

		<property name="result" type="boolean" column="check_result"
			not-null="true"></property>

		<property name="reason" type="string" column="check_reason"
			length="50"></property>

		<many-to-one name="app" class="Application" column="app_id"
			unique="true" not-null="true" lazy="false"></many-to-one>

		<many-to-one name="manager" class="Manager" column="mgr_id"
			not-null="true" lazy="false"></many-to-one>
	</class>
</hibernate-mapping>
